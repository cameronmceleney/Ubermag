# MIF 2.2

SetOptions {
  basename breaking_test
  scalar_output_format %.12g
  scalar_field_output_format {binary 8}
  vector_field_output_format {binary 8}
}

# BoxAtlas for fixedLhs_atlas
Specify Oxs_BoxAtlas:fixedLhs_atlas {
  xrange { 0.0 2e-09 }
  yrange { 0.0 2e-09 }
  zrange { 0.0 2e-09 }
  name fixedLhs
}

# BoxAtlas for freeLhs_atlas
Specify Oxs_BoxAtlas:freeLhs_atlas {
  xrange { 2e-09 1e-06 }
  yrange { 0.0 2e-09 }
  zrange { 0.0 2e-09 }
  name freeLhs
}

# BoxAtlas for driven_atlas
Specify Oxs_BoxAtlas:driven_atlas {
  xrange { 1e-06 1.05e-06 }
  yrange { 0.0 2e-09 }
  zrange { 0.0 2e-09 }
  name driven
}

# BoxAtlas for freeRhs_atlas
Specify Oxs_BoxAtlas:freeRhs_atlas {
  xrange { 1.05e-06 2.046e-06 }
  yrange { 0.0 2e-09 }
  zrange { 0.0 2e-09 }
  name freeRhs
}

# BoxAtlas for fixedRhs_atlas
Specify Oxs_BoxAtlas:fixedRhs_atlas {
  xrange { 2.046e-06 2.048e-06 }
  yrange { 0.0 2e-09 }
  zrange { 0.0 2e-09 }
  name fixedRhs
}

# BoxAtlas for entire_atlas
Specify Oxs_BoxAtlas:entire_atlas {
  xrange { 0.0 2.048e-06 }
  yrange { 0.0 2e-09 }
  zrange { 0.0 2e-09 }
  name entire
}

# MultiAtlas
Specify Oxs_MultiAtlas:main_atlas {
  atlas :fixedLhs_atlas
  atlas :freeLhs_atlas
  atlas :driven_atlas
  atlas :freeRhs_atlas
  atlas :fixedRhs_atlas
  atlas :entire_atlas
  xrange { 0.0 2.048e-06 }
  yrange { 0.0 2e-09 }
  zrange { 0.0 2e-09 }
}

# RectangularMesh
Specify Oxs_RectangularMesh:mesh {
  cellsize { 2e-09 2e-09 2e-09 }
  atlas :main_atlas
}

# FixedZeeman
Specify Oxs_FixedZeeman:static {
  field {0.0 0.0 79577.47154594767}
}

# exchange_A file
Specify Oxs_FileVectorField:exchange_A {
  file exchange_A.ovf
  atlas :main_atlas
}

# exchange_A_norm
Specify Oxs_VecMagScalarField:exchange_A_norm {
    field :exchange_A
}

# ExchangePtwise
Specify Oxs_ExchangePtwise:exchange {
  A exchange_A_norm
}

# m0 file
Specify Oxs_FileVectorField:m0 {
  file m0.omf
  atlas :main_atlas
}

# m0_norm
Specify Oxs_VecMagScalarField:m0_norm {
    field :m0
}

# CGEvolver
Specify Oxs_CGEvolve:evolver {
  fixed_spins {main_atlas fixedLhs fixedRhs}
}

# MinDriver
Specify Oxs_MinDriver {
  evolver :evolver
  mesh :mesh
  Ms :m0_norm
  m0 :m0
  stopping_mxHxm 0.1
}

Destination table mmArchive
Destination mags mmArchive

Schedule DataTable table Stage 1
Schedule Oxs_MinDriver::Magnetization mags Stage 1